{
  "expo": {
    "name": "Plenitude Pilates",
    "slug": "my-app",
    "owner": "thiagoraia",
    "version": "1.0.0",
    "githubUrl": "https://github.com/ThiagoRaia1/Pi-5o-Semestre",
    "icon": "./assets/appIcon.png",
    "backgroundColor": "#ffffff",
    "primaryColor": "#ffffff",
    "orientation": "portrait",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "androidStatusBar": {
      "hidden": true
    },
    "androidNavigationBar": {
      "visible": "immersive"
    },
    "splash": {
      "image": "./assets/splashImage.png",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#ffffff"
      },
      "package": "com.thiagoraia.myapp",
      "softwareKeyboardLayoutMode": "pan"
    },
    "web": {
      "favicon": "./assets/splashImage.png"
    },
    "plugins": [
      "expo-router",
      "expo-secure-store"
    ],
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "ccbea3e2-ae98-450a-8fd0-ca103a7631f1"
      }
    }
  }
}